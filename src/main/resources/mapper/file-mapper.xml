<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.kahyun.mapper.FileMapper">

    <!-- 저장 -->
    <insert id="saveFile" parameterType="list">
        INSERT INTO file
            s_board_seq ,orgName, savedName, size, create_dt
        VALUES
            <foreach collection="list" separator=", " item="file">
                (#{s_board_seq}, #{orgName}, #{savedName}, #{size}, CURRENT_TIME())
            </foreach>
    </insert>

</mapper>